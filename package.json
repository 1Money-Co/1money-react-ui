    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test",
      "commit-msg": "npm run lint:commit"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:es_fix",
      "npm run lint:prettier_fix"
    ],
    "src/**/*.{css,scss,sass,less}": [
      "npm run lint:style_fix",
      "npm run lint:prettier_fix"
    ]
